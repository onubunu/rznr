<h1>Listing settings</h1>

<table>
  <tr>
    <th>Firmlogo</th>
    <th>Firmname</th>
    <th>Firmnameadd</th>
    <th>Firmowner</th>
    <th>Firmstreet</th>
    <th>Firmzipcode</th>
    <th>Firmcity</th>
    <th>Firmcountry</th>
    <th>Firmphone</th>
    <th>Firmfax</th>
    <th>Firmmail</th>
    <th>Firmweb</th>
    <th>Firmtaxno1</th>
    <th>Firmtaxno2</th>
    <th>serviceinfo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @settings.each do |setting| %>
  <tr>
    <td><%= setting.firmlogo %></td>
    <td><%= setting.firmname %></td>
    <td><%= setting.firmnameadd %></td>
    <td><%= setting.firmowner %></td>
    <td><%= setting.firmstreet %></td>
    <td><%= setting.firmzipcode %></td>
    <td><%= setting.firmcity %></td>
    <td><%= setting.firmcountry %></td>
    <td><%= setting.firmphone %></td>
    <td><%= setting.firmfax %></td>
    <td><%= setting.firmmail %></td>
    <td><%= setting.firmweb %></td>
    <td><%= setting.firmtaxno1 %></td>
    <td><%= setting.firmtaxno2 %></td>
    <td><%= setting.serviceinfo %></td>
    <td><%= link_to 'Show', setting %></td>
    <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
    <td><%= link_to 'Destroy', setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Setting', new_setting_path %>
